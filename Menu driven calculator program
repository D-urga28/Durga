
#include <stdio.h>


int isPrime(int num) {
    if (num < 2) {
        return 0; 
    }
    for (int i = 2; i <= num / 2; i++) {
        if (num % i == 0) {
            return 0; 
        }
    }
    return 1; 
}

int main() {
    int x;

    
    printf("Enter a number: ");
    scanf("%d", &x);

    
    if (isPrime(x)) {
        printf("prime\n");
    } 
    
    else {
        if (x % 2 == 0) {
            printf("even\n");
        } else {
            printf("odd\n");
        }
    }

    return 0;
}


